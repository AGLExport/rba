	Constraint HA1051_影響エリアの再調停テスト_1 {
		runtime: true
		IF (A.isDisplayed())
		THEN 
		  IF (B.isDisplayed())
		  THEN !C.isDisplayed()
		  ELSE !D.isDisplayed()
		ELSE
		  IF (E.isDisplayed())
		  THEN !F.isDisplayed()
		  ELSE !G.isDisplayed()
	}
