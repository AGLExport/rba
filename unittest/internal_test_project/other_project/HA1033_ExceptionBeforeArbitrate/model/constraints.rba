Package Constraints {
	
	Constraint test_allocatedContent {
		runtime: true
		(Area_A.allocatedContent() != a1) -> (Area_B.isHidden())
	}
	
	Constraint test_displayingContent {
		runtime: true
		(Area_A.displayingContent() != a1) -> (Area_C.isHidden())
	}
	
	Constraint test_outputtingSound {
		runtime: true
		(Zone_X.outputtingSound() != x1) -> (Zone_Y.isMuted())
	}
	
}
