Package ViewContents {
	
	ViewContent a1 {
		allocatable: [Area_A]
		State NORMAL { priority: STANDARD_VALUE }
		sizeReference: AreaSize
	}
		
	ViewContent b1 {
		allocatable: [Area_B]
		State NORMAL { priority: STANDARD_VALUE }
		sizeReference: AreaSize
	}
	
	ViewContent c1 {
		allocatable: [Area_C]
		State NORMAL { priority: STANDARD_VALUE }
		sizeReference: AreaSize
	}
	
}

Package SoundContents {
	
	SoundContent x1 {
		allocatable: [Zone_X]
		State NORMAL { priority: STANDARD_VALUE }
	}
	
	SoundContent y1 {
		allocatable: [Zone_Y]
		State NORMAL { priority: STANDARD_VALUE }
	}
	
}