Package Displays {
	Display Disp1{
		sizeReference: Size1
		CompositeArea Default1 {
			layout: FixedPositionLayout {
				PositionContainer {
					x:0
					y:0
					basePoint: LEFT_TOP
					areaReference: A1
				}
			}
		}
	}
	Size Size1 {width: 100 height: 100} 
	Area A1 {
		visibility: STANDARD_VALUE
		arbitrationPolicy: LAST_COME_FIRST
		zorder: STANDARD_VALUE
		sizeReference: Size1
	}
	ViewContent V1{
		loserType: NEVER_GIVEUP
		sizeReference: Size1
		State N {
			priority: STANDARD_VALUE
		}
		allocatable: [A1]
	}
	Scene GlobalWithProperty{
		global: true
		int PropertyA : 3
		int PropertyB : 10
	}
	Constraint C1_PropAisOn_GetPropAEqual_10_GetPropBEqual9999 {
		runtime: true
		GlobalWithProperty.isOn() AND GlobalWithProperty.PropertyA.get() = 10 AND GlobalWithProperty.PropertyB.get() = 9999 -> A1.isHidden()
	}
}