Package Model {
	Constraint エリア間で先勝ち {
		runtime: true
		IF (a1.hasComeEarlierThan(b1))
		THEN ! b1.isSounding() 
		ELSE ! a1.isSounding()
	}

	Zone A {
		arbitrationPolicy: PRIORITY_LAST_COME_FIRST
		visibility: > That-of B
	}
	Zone B {
		arbitrationPolicy: PRIORITY_LAST_COME_FIRST
		visibility: MIN_VALUE
	}

	SoundContent a1 {
		allocatable: [ A ]
		State NORMAL { priority: 10 }
	}
	SoundContent b1 {
		allocatable: [ B ]
		State NORMAL { priority: 10 }
	}
}
