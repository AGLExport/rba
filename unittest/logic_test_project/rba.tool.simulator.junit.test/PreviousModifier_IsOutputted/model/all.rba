Package Displays {
	Zone ゾーンA {
		arbitrationPolicy: DEFAULT
		visibility: > That-of ゾーンB
		attenuateValue:1
	}
	Zone ゾーンB {
		arbitrationPolicy: DEFAULT
		visibility: NONE_VALUE
		attenuateValue:2
	}

	SoundContent コンテンツA1{
		allocatable: [
			ゾーンA
		]
		State NORMAL {
			priority: > That-of コンテンツA2.NORMAL
		}
	}
	SoundContent コンテンツB1 {
		allocatable: [
			ゾーンB
		]
		State NORMAL {
			priority: > That-of コンテンツB2.NORMAL
		}
	}
	SoundContent コンテンツA2{
		allocatable: [
			ゾーンA
		]
		State NORMAL {
			priority: STANDARD_VALUE
		}
	}
	SoundContent コンテンツB2 {
		allocatable: [
			ゾーンB
		]
		State NORMAL {
			priority: STANDARD_VALUE
		}
	}

	Constraint PreviousModifier_IsOutputted {
		runtime: true
		(pre)ゾーンA.isOutputted() -> !コンテンツA1.isSounding() AND !コンテンツB1.isSounding()
	}
}
