		node [shape = box];
		{
			Area;

			e1 [label="優先度（高）ゾーン", fontname="Meiryo UI"];
			e2 [label="優先度（低）ゾーン", fontname="Meiryo UI"];
		}
	}
	
	{ rank = same;
		node [shape = ellipse];
		{
