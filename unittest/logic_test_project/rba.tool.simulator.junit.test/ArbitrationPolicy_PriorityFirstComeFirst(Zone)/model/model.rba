Package P1 {
	Zone A1 {
		arbitrationPolicy: PRIORITY_FIRST_COME_FIRST
		visibility: 1
	}
	SoundContent C1 {
		allocatable: [ A1 ]
		State CS_N { priority: STANDARD_VALUE }
	}
	SoundContent C2 {
		allocatable: [ A1 ]
		State CS_N { priority: > That-of C1.CS_N }
	}
	SoundContent C3 {
		allocatable: [ A1 ]
		State CS_N { priority: That-of C2.CS_N }
	}
}
